{
  "multipart": [
    {
      "when": {
        "lit": "true"
      },
      "apply": {
        "model": "contentcontent:block/soul_brazier_lit"
      }
    },
    {
      "when": {
        "lit": "false"
      },
      "apply": {
        "model": "contentcontent:block/brazier"
      }
    },
    {
      "when": {
        "chain": "true"
      },
      "apply": {
        "model": "contentcontent:block/brazier_chain"
      }
    },
    {
      "when": {
        "legs": "true"
      },
      "apply": {
        "model": "contentcontent:block/brazier_legs"
      }
    }
  ]
}